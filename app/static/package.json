{
  "name": "pygoat-client",
  "version": "1.0.0",
  "description": "A React front-end for the WebGoat inspired Pythonic Cyber Security Application, PyGoat",
  "main": "index.js",
  "scripts": {
    "buildComps": "npx babel --watch ./jsx/components --out-dir ./js/components --presets react",
    "buildBase": "npx babel --watch ./jsx/base --out-dir ./js --presets react"
  },
  "comment": "buildComps transpiles from jsx/components, while buildBase draws from jsx/base",
  "keywords": [
    "Python",
    "CyberSecurity",
    "Educational",
    "PyGoat"
  ],
  "author": "James E. Halladay",
  "license": "MIT",
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.10.1",
    "@testing-library/jest-dom": "^5.8.0",
    "@testing-library/react": "^10.0.4",
    "@testing-library/user-event": "^10.4.0",
    "babel-cli": "^6.26.0",
    "babel-jest": "^26.0.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^9.1.2",
    "jest": "^26.0.1",
    "react-test-renderer": "^16.13.1"
  }
}
